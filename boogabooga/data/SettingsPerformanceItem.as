/** SettingsPerformanceItem  *	---------------------------------------------------------------------------- *  *	@desc:  *		Main data index for the game. It's a singleton that you can access from anywhere.  *	@author:   *		Christian Widodo, [christian@avatarlabs.com]  *	---------------------------------------------------------------------------- */	package com.boogabooga.data	{				public class SettingsPerformanceItem extends Object		{			public var id:int;			public var value:int;			public var toggled:Boolean;			public var alwaysOff:Boolean;						/** Constructor			  *	---------------------------------------------------------------------- */				public function SettingsPerformanceItem( id:int, value:int, toggled:Boolean, alwaysOff:Boolean )				{					this.id = id;					this.value = value;					this.toggled = toggled;					this.alwaysOff = alwaysOff;				}						}			}