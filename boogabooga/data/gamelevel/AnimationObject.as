/** AnimationObject  *	---------------------------------------------------------------------------- *  *	@desc:  *		AnimationObject data class  *	@author:   *		Christian Widodo, [christian@avatarlabs.com]  *	---------------------------------------------------------------------------- */	package com.boogabooga.data.gamelevel	{		import flash.events.Event;		import flash.events.EventDispatcher;		import flash.events.IOErrorEvent;		import flash.events.FullScreenEvent;		import flash.events.ProgressEvent;		import flash.external.ExternalInterface;		import flash.geom.Point;		import flash.net.URLLoader;		import flash.net.URLRequest;		import flash.net.URLRequestMethod;		import flash.net.URLVariables;				import com.avatarlabs.utils.UtilFunctions;		import com.avatarlabs.utils.events.CustomEvent;		import com.avatarlabs.utils.events.EventNotificationCenter;		import com.avatarlabs.utils.console.ConsoleBroadcaster;				import com.boogabooga.data.gamelevel.AbstractGameObject;		import com.boogabooga.data.maingame.*;				public class AnimationObject extends Object		{			protected var _id:uint;			protected var _tileWidth:uint;			protected var _tileHeight:uint;			protected var _offset:Object;			protected var _animationCount:uint;			protected var _animationEvent:uint;			protected var _assetURL:String;			protected var _cacheId:String;			protected var _customPosition:Point;			protected var _customPositions:Vector.<Point>;			protected var _flippedCustomPosition:Point;			protected var _flippedCustomPositions:Vector.<Point>;			protected var _soundDataIds:Array;			protected var _soundEvent:uint;			protected var _idleFrames:uint;			protected var _startIdleFrame:uint;						/** Constructor			  *	---------------------------------------------------------------------- */				public function AnimationObject()				{					init();									}							/** init			  *	---------------------------------------------------------------------- */				public function init():void				{					_tileWidth = 128;					_tileHeight = 128;					_animationCount = 0;					_animationEvent = 0;					_assetURL = "";					_customPosition = new Point();					_customPositions = new Vector.<Point>;					_flippedCustomPosition = new Point();					_flippedCustomPositions = new Vector.<Point>;					_soundDataIds = new Array();					_offset = new Object();					_idleFrames = 0;				}								public function set id( i:uint ):void { _id = i; }				public function get id():uint { return _id; }								public function set tileWidth( u:uint ):void { _tileWidth = u; }				public function get tileWidth():uint { return _tileWidth; }								public function set tileHeight( u:uint ):void { _tileHeight = u; }				public function get tileHeight():uint { return _tileHeight; }								public function set offset( o:Object ):void { _offset = o; }				public function get offset():Object { return _offset; }								public function set animationCount( u:uint ):void { _animationCount = u; }				public function get animationCount():uint { return _animationCount; }								public function set animationEvent( u:uint ) :void { _animationEvent = u; }				public function get animationEvent():uint { return _animationEvent; }								public function set assetURL( s:String ):void { _assetURL = s; }				public function get assetURL():String { return _assetURL; }								public function set cacheId( s:String ):void { _cacheId = s; }				public function get cacheId():String { return _cacheId; }								public function set customPosition( p:Point ):void { _customPosition = p; }				public function get customPosition():Point { return _customPosition; }								public function set customPositions( v:Vector.<Point> ):void { _customPositions = v; }				public function get customPositions():Vector.<Point> { return _customPositions; }								public function set flippedCustomPosition( p:Point ):void { _flippedCustomPosition = p; }				public function get flippedCustomPosition():Point { return _flippedCustomPosition; }								public function set flippedCustomPositions( v:Vector.<Point> ):void { _flippedCustomPositions = v; }				public function get flippedCustomPositions():Vector.<Point> { return _flippedCustomPositions; }								public function set soundDataIds( a:Array ):void { _soundDataIds = a; }				public function get soundDataIds():Array { return _soundDataIds; }								public function set soundEvent( u:uint ):void { _soundEvent = u; }				public function get soundEvent():uint { return _soundEvent; }								public function set idleFrames( u:uint ):void { _idleFrames = u; _startIdleFrame = _idleFrames > 0 ? _animationCount - _idleFrames : 0; }				public function get idleFrames():uint { return _idleFrames; }								public function get startIdleFrame():uint { return _startIdleFrame; }								public function clone():AnimationObject				{					var cloned:AnimationObject = UtilFunctions.clone(this) as AnimationObject;										return cloned;				}		}			}