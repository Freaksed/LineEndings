/** Gate  *	---------------------------------------------------------------------------- *  *	@desc:  *		Gate data class  *	@author:   *		Christian Widodo, [christian@avatarlabs.com]  *	---------------------------------------------------------------------------- */	package com.boogabooga.data.gamelevel	{		import flash.events.Event;		import flash.events.EventDispatcher;		import flash.events.IOErrorEvent;		import flash.events.FullScreenEvent;		import flash.events.ProgressEvent;		import flash.external.ExternalInterface;		import flash.net.URLLoader;		import flash.net.URLRequest;		import flash.net.URLRequestMethod;		import flash.net.URLVariables;				import com.avatarlabs.utils.UtilFunctions;		import com.avatarlabs.utils.events.CustomEvent;		import com.avatarlabs.utils.events.EventNotificationCenter;		import com.avatarlabs.utils.console.ConsoleBroadcaster;				import com.boogabooga.data.gamelevel.AbstractGameObject;		import com.boogabooga.data.gamelevel.AnimationObject;		import com.boogabooga.data.maingame.*;				public class Gate extends AbstractGameObject		{			protected var _animationObject:AnimationObject;			protected var _health:Number;			protected var _maxHealth:Number;						/** Constructor			  *	---------------------------------------------------------------------- */				public function Gate()				{					init();				}								public function set animationObject( a:AnimationObject ):void { _animationObject = a; }				public function get animationObject():AnimationObject { return _animationObject; }							/** set/get health			  *	---------------------------------------------------------------------- */				public function set health( n:Number ):void				{					_health = n;										if( isNaN(_maxHealth) )						_maxHealth = _health;										//trace( "_maxHealth: "+_maxHealth );				}				public function get health():Number { return _health; }							/** get maxHealth			  *	---------------------------------------------------------------------- */			  	public function set maxHealth( n:Number ) { _maxHealth = n; }				public function get maxHealth():Number { return _maxHealth; }							/** init			  *	---------------------------------------------------------------------- */				override public function init():void				{					super.init();										_debug = true;									}								public function clone():Gate				{					var cloned:Gate = UtilFunctions.clone( this ) as Gate;										return cloned;				}		}				}