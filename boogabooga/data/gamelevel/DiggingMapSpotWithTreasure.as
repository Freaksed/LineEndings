/** DiggingMapSpotWithTreasure  *	---------------------------------------------------------------------------- *  *	@desc:  *		DiggingMapSpotWithTreasure data class  *	@author:   *		Christian Widodo, [christian@avatarlabs.com]  *	---------------------------------------------------------------------------- */	package com.boogabooga.data.gamelevel	{				public class DiggingMapSpotWithTreasure extends Object		{			public var r:int;			public var c:int;			public var tid:int;			public var wid:int;			public var gid:int;			public var d:int;						/** Constructor			  *	---------------------------------------------------------------------- */				public function DiggingMapSpotWithTreasure()				{					r = -1;					c = -1;					tid = 0;					wid = 0;					gid = 0;					d = 0;				}							/** toString			  *	---------------------------------------------------------------------- */				public function toString():String				{					return "row: "+r+", column: "+c+", dug: "+d+", treasure_id: "+tid+", weapon_id: "+wid+", god_id: "+gid;				}						}			}