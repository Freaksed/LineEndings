/** BitmapDataObject *	---------------------------------------------------------------------------- */ 	package com.avatarlabs.utils.cache	{		import flash.display.BitmapData;		import flash.system.System;				import com.avatarlabs.utils.console.ConsoleBroadcaster;				public class BitmapDataObject extends Object		{			protected var _id:String;			protected var _bitmapData:BitmapData;			protected var _overwrite:Boolean;						/** Constructor			  *	---------------------------------------------------------------------------- */				public function BitmapDataObject( id:String, bitmapData:BitmapData, overwrite:Boolean=false )				{					_id = id;					_bitmapData = bitmapData.clone();					_overwrite = overwrite;										bitmapData.dispose();				}							/** get id			  *	---------------------------------------------------------------------------- */			  	public function get id():String				{					return _id;				}							/** set/get bitmapData			  *	---------------------------------------------------------------------------- */				public function set bitmapData( b:BitmapData ):void				{					if( _bitmapData )						_bitmapData.dispose();										_bitmapData = b.clone();				}				public function get bitmapData():BitmapData				{					return _bitmapData;				}							/** get overwrite			  *	---------------------------------------------------------------------------- */				public function get overwrite():Boolean				{					return _overwrite;				}								public function destroy():void				{					if( _bitmapData )						_bitmapData.dispose();				}		}			}