/** SoundEffectClip  *	---------------------------------------------------------------------------- *  *	@desc:  *		SoundEffectClip class  *	@developer: Christian Widodo, [christian@avatarlabs.com]  *	---------------------------------------------------------------------------- */  	package com.avatarlabs.utils.sound	{		import flash.events.*;		import flash.errors.*;				import com.avatarlabs.utils.events.EventNotificationCenter;		import com.avatarlabs.utils.console.ConsoleBroadcaster;		import flash.media.Sound;		import flash.media.SoundChannel;		import flash.media.SoundTransform;				public class SoundEffectClip extends EventDispatcher		{			private var _sound:Sound;			private var _soundChannel:SoundChannel;			private var _soundTransform:SoundTransform;			private var _loop:Boolean;			private var _id:String;			private var _soundId:String;						/** Constructor			  *	---------------------------------------------------------------------- */				public function SoundEffectClip()				{									}							/** get sound			  *	---------------------------------------------------------------------- */				public function get sound():Sound				{					return this._sound;				}							/** get soundChannel			  *	---------------------------------------------------------------------- */				public function get soundChannel():SoundChannel				{					return this._soundChannel;				}							/** set/get soundTransform			  *	---------------------------------------------------------------------- */				public function set soundTransform( s:SoundTransform ):void				{					this._soundTransform = s;				}				public function get soundTransform():SoundTransform				{					return this._soundTransform;				}							/** set/get loop			  *	---------------------------------------------------------------------- */				public function set loop( l:Boolean ):void				{					this._loop = l;				}				public function get loop():Boolean				{					return this._loop;				}							/** set/get id			  *	---------------------------------------------------------------------- */				public function set id( anId:String ):void				{					this._id = anId;				}				public function get id():String				{					return this._id;				}							/** set/get soundId			  *	---------------------------------------------------------------------- */				public function set soundId( sId:String ):void				{					this._soundId = sId;				}				public function get soundId():String				{					return this._soundId;				}		}	}