/**	DynamicUINavigationButton *	------------------------------------------------------------------------------------ * *	Updates: 080820-James Safechuck: added functions releaseListeners/init *	------------------------------------------------------------------------------------ *	 *	------------------------------------------------------------------------------------ */	package com.avatarlabs.utils.navigation	{		import flash.events.*;		import flash.display.MovieClip;		import com.avatarlabs.utils.console.*;		import com.avatarlabs.utils.userinterface.PlayToRewindTo;				public class DynamicUINavigationButton extends UINavigationButton		{						/**	constructor			  *	------------------------------------------------------------ */				public function DynamicUINavigationButton()				{									}							/**	set/get dynamicClip			  *	------------------------------------------------------------ */				public function set dynamicClip( m:MovieClip ):void { _dynamicClip = m; }				public function get dynamicClip():MovieClip { return _dynamicClip; }							/**	init			  *	------------------------------------------------------------ */				override public function init( event:Event=null ):void				{					//ConsoleBroadcaster.broadcast( "DynamicUINavigationButton","setActive( "+a+" )");									}							/**	dynamicInit			  *	------------------------------------------------------------ */				public function dynamicInit():void				{					trace("DynamicUINavigationButton: init");										_dynamicClip.gotoAndStop(1);										_dynamicClip.buttonMode = true;					//_dynamicClip.mouseEnabled = false;					_dynamicClip.tabEnabled = false;										_ptrt = new PlayToRewindTo( _dynamicClip );										_dynamicClip.addEventListener( MouseEvent.CLICK, handleClick, false, 0, true );					_dynamicClip.addEventListener( MouseEvent.MOUSE_OVER, focus, false, 0, true );					_dynamicClip.addEventListener( MouseEvent.MOUSE_OUT, blur, false, 0, true );				}		}			}