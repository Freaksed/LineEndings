/** PartigenParticle *	---------------------------------------------------------------------------- */ 	package com.avatarlabs.utils.particles	{		public class PartigenParticle extends Object		{			protected var _id:String;			protected var _assetURL:String;			protected var _xml:XML;						/** Constructor			 *	---------------------------------------------------------------------------- */				public function PartigenParticle()				{					init();				}							/** set/get id			  *	---------------------------------------------------------------------- */				public function set id( _id:String ):void { this._id = _id; }				public function get id():String { return this._id; }							/** set/get assetURL			  *	---------------------------------------------------------------------- */				public function set assetURL( u:String ):void { this._assetURL = u; }				public function get assetURL():String { return this._assetURL; }							/** set/get xml			  *	---------------------------------------------------------------------- */				public function set xml( x:XML ):void { this._xml = x; }				public function get xml():XML { return this._xml; }							/** init			  *	---------------------------------------------------------------------- */				public function init():void				{					this._id = "";					this._assetURL = "";				}		}			}